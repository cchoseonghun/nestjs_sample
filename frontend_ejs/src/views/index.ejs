<body>
  
  <h1>Front-End with ejs</h1>

  <% if (locals.component === 'map/naver/default') { %>
  <%- include('map/naver/default') %>
  <% } else if (locals.component === 'map/naver/current') { %>
  <%- include('map/naver/current') %>
  <% } else if (locals.component === 'map/naver/target') { %>
  <%- include('map/naver/target') %>
  <% } else { %>
  <%- include('./boards/list') -%>
  <% } %>
  
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
